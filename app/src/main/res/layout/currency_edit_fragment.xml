<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bind="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="currencyName" type="String"/>
        <variable name="currencyCode" type="String"/>
        <variable name="symbolChecker" type="com.vividprojects.protoplanner.Utils.SymbolChecker"/>
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include layout="@layout/currency_edit_fragment_base"
            bind:currencyName="@{currencyName}"
            bind:currencyCode="@{currencyCode}"
            bind:symbolChecker="@{symbolChecker}"/>

    </LinearLayout>
</layout>